<div class="card my-3">
  <div class="card-header">
    <h2 class="h5">
      <%= dgettext("accounts", "Edit account") %>
    </h2>
  </div>

  <div class="card-body">
    <%= render "_form.html", Map.put(assigns, :action, Routes.account_path(@conn, :update, @account)) %>
  </div>
</div>

<div class="mt-3">
  <%= link dgettext("accounts", "Back"), to: Routes.account_path(@conn, :index) %>
</div>
